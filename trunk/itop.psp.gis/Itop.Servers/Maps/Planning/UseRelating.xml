			"LinkUID",
			"UsePropertyUID",
			"SvgUid",
			"MenuUID",
			"Obligate1",
			"Obligate2",
			"Obligate3",
			"Obligate4"
		)
		VALUES
		(
			#UID#,
			#UseID#,
			#LinkUID#,
			#UsePropertyUID#,
			#SvgUid#,
			#MenuUID#,
			#Obligate1#,
			#Obligate2#,
			#Obligate3#,
			#Obligate4#
		)
	</insert>
	<delete id='DeleteUseRelating' parameterClass='UseRelating'>
