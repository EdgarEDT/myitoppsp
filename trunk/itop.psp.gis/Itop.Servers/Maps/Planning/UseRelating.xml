			"Obligate2" = #Obligate2#,
			"Obligate3" = #Obligate3#,
			"Obligate4" = #Obligate4#
		WHERE
			UID = #UID#
	</update>
	<select id='SelectUseRelatingList' resultMap='UseRelatingResult' parameterClass='UseRelating'>
		SELECT
			"UID",
			"UseID",
			"LinkUID",
			"UsePropertyUID",
			"SvgUid",
			"MenuUID",
