    AND a.ID = c.ParentID AND c.Col4 = 'pw-pb'
    and a.ID = d.ParentID AND d.Col4 = 'pw-kg' $value$) and Typeqf='pw-pb'
  </select>
  <!--统计十二五配网变其他元件动态投资总额-->
